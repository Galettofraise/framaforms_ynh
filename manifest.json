{
  "name": "Framaforms",
  "id": "framaforms",
  "packaging_format": 1,
  "description": {
    "en": "Framaforms helps you create online webforms and surveys"
  },
  "version": "0.0.0+190614-ynh1",
  "url": "https://framagit.org/framasoft/framaforms",
  "license": "GPL-2.0-only",
  "requirements": {
		"yunohost": ">= 3.5"
  },
  "maintainer": {
    "name": "ljf",
    "email": "ljf+framaforms_ynh@reflexlibre.net",
    "url": "https://reflexlibre.net"
  },
  "multi_instance": true,
  "services": [
    "nginx",
    "php5-fpm",
    "mysql"
  ],
  "arguments": {
    "install": [
      {
        "name": "domain",
        "ask": {
          "en": "Choose a domain for Framaforms"
        },
        "example": "domain.org",
        "type": "domain"
      },
      {
        "name": "path",
        "ask": {
          "en": "Choose a path for Framaforms"
        },
        "type": "path",
        "example": "/poll",
        "default": "/poll"
      },
      {
        "name": "admin",
        "ask": {
          "en": "Choose an admin user"
        },
        "type": "user",
        "example": "johndoe"
      },
      {
        "name": "is_public",
        "ask": {
          "en": "Are visitors who do not have a YunoHost account on this server allowed to register and create forms ?"
        },
        "type": "boolean",
        "default": true
      }
    ]
  }
}
